{
    "name": "EdunextTool",
    "version": "1.0.0",
    "description": "Build an Extension!",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["https://fu-edunext.fpt.edu.vn/*", "https://github.com/Cuongtran6969", "https://www.youtube.com/channel/UCcY1MxmE0lqLKWPsvaYd-vw"],
        "css": ["popup.css"],
        "js": ["popup.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
      "48": "/img/icon.jpg",
      "128": "/img/icon.jpg"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ]
  }